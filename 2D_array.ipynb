   "source": [
    "#Program to create an image using 2D array.\n",
    "import cv2 as c\n",
    "import numpy as np\n",
    "from PIL import Image\n",
    "array=np.zeros([100,200,3],dtype=np.uint8)\n",
    "array[:,:100]=[255,130,0]\n",
    "array[:,100:]=[0,0,255]\n",
    "img=Image.fromarray(array)\n",
    "img.save('flower5.jpg')\n",
    "img.show()\n",
    "c.waitKey(0)"
   ]
  }
